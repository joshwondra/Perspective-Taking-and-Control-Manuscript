get.percent <- function(dv, group) {
    result <- by(dv, group, function(x){sum(x)/length(x)})
    result <- round(result, digits=2)
    result <- result*100
    return(result)
}

get.med <- function(dv, group) {
    result <- by(dv, group, median, na.rm=TRUE)
}

get.ci <- function(object, row) {
    lb <- round(confint(object)[row,1], digits=2)
    ub <- round(confint(object)[row,2], digits=2)
    result <- paste(lb, ub, sep=', ')
    return(result)
}


